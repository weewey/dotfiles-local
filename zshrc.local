export ECLIPSE_HOME="$HOME/java-oxygen/Eclipse.app/Contents/Eclipse/"

#powerline
. /Users/YewWee/Library/Python/2.7/lib/python/site-packages/powerline/bindings/zsh/powerline.zsh

plugins=(git colorize github virtualenv node npm z extract pip python brew osx zsh-syntax-highlighting)
