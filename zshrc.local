if [ "$TMUX" = "" ]; then tmux; fi

set shell=bash\ -i

export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

. ~/z.sh

#gcloud cli
source ~/google-cloud-sdk/completion.zsh.inc

source ~/google-cloud-sdk/path.zsh.inc

#kubectl code completion
